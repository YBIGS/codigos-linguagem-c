#include <iostream>
using namespace std;
 
int main(void) {
	
	int V[4];   // Define an array of four integers
        int i;
	// Get inputs for the array elements
	for (i=0; i<4; i++) {
    	cin >> V[i];
	}
    
	int soma = 0;
	// Write the logic to add these integers here:
    for (i=0; i<4; i++){
        soma = soma + V[i];
    }
 
	cout << soma << endl;// Print the sum
    
	return 0;
}
