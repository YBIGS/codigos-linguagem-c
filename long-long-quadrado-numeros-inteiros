#include <iostream>
using namespace std;

int main() {
    double a, b;

    setlocale(LC_ALL, "Portuguese");

    cout << "\n" << "Quadrado de números inteiros:" << "\n" << "\n";
    cout << "Determine o valor envolvidos no cálculo matemático." << "\n";
    cout << "Ou a quantidade de inteiros devem ser maiores que -999 e menores que 999" << "\n" << "\n";

    cout << "Determine o valor: ";
    cin >> a;

    cout << "\n";

    cout << "Resultado: "<< a * a << "\n" <<endl;
}
