#include <iostream>
using namespace std;

int main(){
    int i, j, n, m;
     //tamanho matriz
    cin >> n >> m;

    int A[n][m] ;
    int B[n][m] ;
    int C[n][m] ;

    for(i = 0; i < n; i++){
      for(j = 0; j < m; j++){
          cin >> A[i][j];
        }
    }
    for(i = 0; i < n; i++){
        for(j = 0; j < m; j++){
            cin >> B[i][j];
        }
    }
    for(i = 0; i < n; i++){
        for(j = 0; j < m; j++){
          C[i][j] = A[i][j] - B[i][j];
            cout << C[i][j] << " ";
        }
        cout << endl;
    }

}
